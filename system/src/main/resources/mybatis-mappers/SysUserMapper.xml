			<if test="userName != null">
				`user_name`,
		  	</if>
			<if test="password != null">
				`password`,
		  	</if>
			<if test="actualName != null">
				`actual_name`,
		  	</if>
			<if test="headImgUrl != null">
				`head_img_url`,
		  	</if>
			<if test="phone != null">
				`phone`,
		  	</if>
			<if test="telePhone != null">
				`tele_phone`,
		  	</if>
			<if test="eMail != null">
				`e_mail`,
		  	</if>
			<if test="birthday != null">
				`birthday`,
		  	</if>
			<if test="sex != null">
				`sex`,
		  	</if>
			<if test="age != null">
				`age`,
		  	</if>
			<if test="status != null">
				`status`,
		  	</if>
			<if test="createTime != null">
				`create_time`,
		  	</if>
			<if test="updateTime != null">
				`update_time`,
		  	</if>
			<if test="createUser != null">
				`create_user`,
		  	</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id},
			</if>
			<if test="userName != null">
				#{userName},
			</if>
			<if test="password != null">
				#{password},
			</if>
			<if test="actualName != null">
				#{actualName},
			</if>
			<if test="headImgUrl != null">
				#{headImgUrl},
			</if>
			<if test="phone != null">
				#{phone},
			</if>
			<if test="telePhone != null">
				#{telePhone},
			</if>
			<if test="eMail != null">
				#{eMail},
			</if>
			<if test="birthday != null">
				#{birthday},
			</if>
			<if test="sex != null">
				#{sex},
			</if>
			<if test="age != null">
				#{age},
			</if>
			<if test="status != null">
				#{status},
			</if>
